<% if (danger.length !== 0) { %>
<div class="alert alert-danger alert-dismissible fade show text-center m-1 p-1" role="alert">
  <%= danger %>
  <button type="button" class="btn-close py-2 px-1" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<% } %> <% if (warning.length !== 0) { %>
<div class="alert alert-warning alert-dismissible fade show text-center m-1 p-1" role="alert">
  <%= warning %>
  <button type="button" class="btn-close py-2 px-1" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<% } %> <% if (success.length !== 0) { %>
<div class="alert alert-success alert-dismissible fade show text-center m-1 p-1" role="alert">
  <%= success %>
  <button type="button" class="btn-close py-2 px-1" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<% } %> <% if (error.length !== 0) { %>
<div class="alert alert-danger alert-dismissible fade show text-center m-1 p-1" role="alert">
  <%= error.name %>
  <button type="button" class="btn-close py-2 px-1" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<% } %>
